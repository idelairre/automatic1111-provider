{
  "name": "comfyui-provider",
  "version": "0.1.1",
  "description": "A TypeScript provider for the Vercel AI SDK that enables image generation using ComfyUI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "artificial-intelligence",
    "stable-diffusion",
    "comfyui",
    "image-generation",
    "vercel-ai-sdk",
    "typescript",
    "ai-provider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idelairre/comfyui-provider"
  },
  "homepage": "https://github.com/idelairre/comfyui-provider#readme",
  "bugs": {
    "url": "https://github.com/idelairre/comfyui-provider/issues"
  },
  "author": "idelairre",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "ai": "^5.0.0"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.1"
  },
  "devDependencies": {
    "@size-limit/file": "^11.2.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.5",
    "ai": "^5.0.5",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "size-limit": "^11.2.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.5"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "clean": "rm -rf dist",
    "prepare": "husky install"
  }
}